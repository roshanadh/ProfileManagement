<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ProfileManagement</display-name>
  
  <servlet>
  	<servlet-name>IndexController</servlet-name>
  	<servlet-class>np.com.roshanadhikary.profilems.controllers.IndexController</servlet-class>
  </servlet>
    
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<servlet-class>np.com.roshanadhikary.profilems.controllers.UserController</servlet-class>
  </servlet>
    
  <servlet>
  	<servlet-name>SignInController</servlet-name>
  	<servlet-class>np.com.roshanadhikary.profilems.controllers.SignInController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>IndexController</servlet-name>
  	<url-pattern></url-pattern>
  </servlet-mapping>
    
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/users/*</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>SignInController</servlet-name>
  	<url-pattern>/signin</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>SignInController</servlet-name>
  	<url-pattern>/signout</url-pattern>
  </servlet-mapping>
</web-app>